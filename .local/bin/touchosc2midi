#!/usr/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'touchosc2midi==0.0.8','console_scripts','touchosc2midi'
__requires__ = 'touchosc2midi==0.0.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('touchosc2midi==0.0.8', 'console_scripts', 'touchosc2midi')()
    )
