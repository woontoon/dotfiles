#!/usr/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'python-rtmidi==0.5b1','console_scripts','osc2midi'
__requires__ = 'python-rtmidi==0.5b1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-rtmidi==0.5b1', 'console_scripts', 'osc2midi')()
    )
